{
    "name": "atmaliance/yii2-atm-keycloak-entity",
    "description": "Package",
    "type": "yii2-extension",
    "keywords": ["yii2","extension","atm72","keycloak","auth","sdk","api","entity"],
    "license": "MIT",
    "authors": [
        {
            "name": "Vitaliy Popov",
            "email": "popov.v.o@atm72.ru",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4",
        "yiisoft/yii2": "2.*",
        "phpdocumentor/reflection-docblock": "^5.3",
        "symfony/property-access": "^5.4",
        "symfony/property-info": "^5.4",
        "symfony/serializer": "^5.4",
        "doctrine/annotations": "^1.13",
        "devatmaliance/keycloak-admin-client": "dev-develop"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "atmaliance\\yii2_keycloak_entity\\": "src"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
            "type": "vcs",
            "url": "https://github.com/devatmaliance/keycloak-admin-client"
        }
    ],
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    }
}
